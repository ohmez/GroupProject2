<article id="main">
    <header>
        <h2>Account Info!</h2>

    </header>
</article>

<section class="wrapper style5">
    <div class="inner">
        {{#each id}}
            <div class="table-wrapper">
                <table>
                    <thead>
                        <tr>
                            <th class='tableSetup'>Category</th>
                            <th class="text-center">Customer Data</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td class='tableSetup'>First Name</td>
                            <td>{{firstName}}</td>
                        </tr>
                        <tr>
                            <td class='tableSetup'>Middle Name</td>
                            <td>{{middleName}}</td>
                        </tr>
                        <tr>
                            <td class='tableSetup'>Last Name</td>
                            <td>{{lastName}}</td>
                        </tr>
                        <tr>
                            <td class='tableSetup'>Email</td>
                            <td>{{email}}</td>
                        </tr>
                        <tr>
                            <td class='tableSetup'>Password</td>
                            <td>{{password}}</td>
                        </tr>
                            <td class='tableSetup'>Birthday</td>
                            <td>{{birthday}}</td>
                        </tr>
                            <td class='tableSetup'>Phone Number</td>
                            <td>{{phoneNumber}}</td>
                        </tr>
                            <td class='tableSetup'>Address</td>
                            <td>{{address}}</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        {{/each}}
    </div>
</section>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/index.js"></script>